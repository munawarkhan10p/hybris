<audit-report-config name="testConfig" auditRecordsProvider="auditRecordsProvider">
	<given-root-type>Product</given-root-type>
	<types>
		<type code="Product">
			<atomic-attributes>
				<atomic-attribute qualifier="code"/>
				<atomic-attribute qualifier="name"/>
				<atomic-attribute qualifier="description"/>
				<atomic-attribute qualifier="catalogVersion"/>
				<atomic-attribute qualifier="ean"/>
				<atomic-attribute qualifier="manufacturerName"/>
				<atomic-attribute qualifier="remarks"/>
				<atomic-attribute qualifier="onlinedate"/>
				<atomic-attribute qualifier="offlinedate"/>
				<atomic-attribute qualifier="approvalstatus"/>
				<atomic-attribute qualifier="priceQuantity"/>
				<atomic-attribute qualifier="minOrderQuantity"/>
				<atomic-attribute qualifier="maxOrderQuantity"/>
			</atomic-attributes>
		</type>
	</types>
</audit-report-config>
